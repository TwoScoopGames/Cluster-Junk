{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": [
            1
        ]
    },
    "env": {
        "browser": true,
        "commonjs": true
    },
    "extends": "eslint:recommended"
}
